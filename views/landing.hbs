<div class="container bg-primary text-white">
    <h3>List of movies, root is {{root}}.</h3>
</div>
<div class="container bg-primary text-white">
    <ul class="p-3 text-lg">
        {{#each movies}}
            <a class="btn-outline-secondary row p-5 justify-content-center" href="{{../root}}/movies/{{tvid}}">
                {{name}}
            </a>
        {{/each}}
    </ul>
</div>